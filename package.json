{
  "name": "wasm-instantiate-streaming",
  "version": "1.0.0",
  "description": "Ponyfill of WebAssembly.instantiateStreaming",
  "main": "index.js",
  "scripts": {
    "prepare": "browserify -t uglifyify browser.js -o build/browser.js",
    "build:no-deps": "deps/wabt/bin/wat2wasm test/fixtures/no-deps.wat -o test/fixtures/no-deps.wasm",
    "build:with-deps": "deps/wabt/bin/wat2wasm test/fixtures/with-deps.wat -o test/fixtures/with-deps.wasm",
    "pretest": "npm-run-all build:no-deps build:with-deps",
    "test": "mocha test/nodejs",
    "pretest:browser": "(static-server test/fixtures --no-nocache --port 7777 --cors '*' &)",
    "test:browser": "mochify --wd test/browser",
    "posttest:browser": "./scripts/kill-by-port 7777"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Leko/instantiate-streaming-ponyfill.git"
  },
  "keywords": [
    "wasm",
    "WebAssembly",
    "polyfill",
    "ponyfill",
    "instantiateStreaming"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leko/instantiate-streaming-ponyfill/issues"
  },
  "homepage": "https://github.com/Leko/instantiate-streaming-ponyfill#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "browserify": "^16.2.0",
    "es6-promisify": "^6.0.0",
    "mocha": "^5.1.1",
    "mochify": "^5.6.0",
    "node-fetch": "^2.1.2",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.5.0",
    "static-server": "^2.2.1",
    "uglifyify": "^4.0.5"
  }
}
